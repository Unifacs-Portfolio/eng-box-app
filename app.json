{
  "expo": {
    "name": "Eng Box",
    "slug": "eng-box3",
    "owner": "sinvalluz",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#767676"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#767676"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.edzao.EngBox"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#767676",
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash.png",
            "backgroundColor": "#767676"
          },
          "imageWidth": 500
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photoPermission": "The app access your photos to let you share them with your friends."
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "e707082a-cb01-4fc2-aba1-f40e3a73bb40"
      }
    }
  }
}
